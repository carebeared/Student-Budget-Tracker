<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal" android:layout_width="match_parent"
        android:layout_height="70dp">
        <Button
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:id="@+id/buttona_a"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="10dp"
            android:textColor="#ffffff"
            android:background="@drawable/food111"/>
    <Button
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:text="Fooding"
        android:id="@+id/buttona"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#ffffff"
        android:background="#DC143C" />
</LinearLayout>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal" android:layout_width="match_parent"
        android:layout_height="55dp">
        <Button
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:id="@+id/buttona_bb"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="10dp"
            android:textColor="#ffffff"
            android:background="@drawable/mobile111"/>
    <Button
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:text="Recharge"
        android:id="@+id/buttonb"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#ffffff"
        android:background="#DC143C" />
        </LinearLayout>
        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:orientation="horizontal" android:layout_width="match_parent"
            android:layout_height="55dp">
            <Button
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:id="@+id/buttona_cc"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="10dp"
                android:textColor="#ffffff"
                android:background="@drawable/cart11"/>
    <Button
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:text="Shopping"
        android:id="@+id/buttonc"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#ffffff"
        android:background="#DC143C" />
            </LinearLayout>
            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="horizontal" android:layout_width="match_parent"
                android:layout_height="55dp">
                <Button
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:id="@+id/buttona_dd"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="25dp"
                    android:layout_marginRight="10dp"
                    android:textColor="#ffffff"
                    android:background="@drawable/ground111"/>
    <Button
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:text="Travel"
        android:id="@+id/buttond"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#ffffff"
        android:background="#DC143C" />
                </LinearLayout>
                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:orientation="horizontal" android:layout_width="match_parent"
                    android:layout_height="55dp">
                    <Button
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:id="@+id/buttona_ee"
                        android:layout_marginBottom="5dp"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="4dp"
                        android:textColor="#ffffff"
                        android:background="@drawable/cat"/>
    <Button
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:text="Others"
        android:id="@+id/buttone"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#ffffff"
        android:background="#DC143C" />
                    </LinearLayout>
</LinearLayout>